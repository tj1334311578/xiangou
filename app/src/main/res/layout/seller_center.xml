<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    android:background="@color/color_pink_fb4a89"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:background="@color/color_pink_fb4a89"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/seller_center_headRL"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:layout_alignTop="@+id/seller_center_userImg"
            android:id="@+id/seller_center_return"
            android:layout_marginStart="13dp"
            android:src="@mipmap/return_icon"/>
        <com.example.administrator.xiangou.tool.SelectImageView
            android:layout_width="65dp"
            app:ImageType="TYPE_CIRCLE"
            android:id="@+id/seller_center_userImg"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:layout_height="65dp"
            android:src="@mipmap/girl_h"
            />
        <ImageView
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="20dp"
            android:id="@+id/seller_center_userMessage"
            android:layout_alignTop="@id/seller_center_userImg"
            android:src="@mipmap/personal_new_icon"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="伊拉克店铺"
            android:id="@+id/seller_center_userName"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/seller_center_userImg"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="建立于2016.06"
            android:textSize="8sp"
            android:layout_marginBottom="10dp"
            android:textColor="@color/white"
            android:layout_below="@id/seller_center_userName"
            android:layout_centerHorizontal="true"
            android:id="@+id/seller_center_createTime"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="本月："
            android:textSize="14sp"
            android:textColor="@color/white"
            android:layout_alignStart="@id/seller_center_return"
            android:layout_below="@id/seller_center_createTime"/>
        <com.example.administrator.xiangou.tool.DrawableTextView
            android:layout_width="wrap_content"
            android:id="@+id/seller_center_fundDetails"
            android:text="资金明细"
            android:paddingStart="15dp"
            android:paddingEnd="6dp"
            android:drawablePadding="5dp"
            app:rightDrawable="@mipmap/mine_showright"
            app:rightDrawableWidth="7dp"
            app:rightDrawableHeight="15dp"
            android:layout_below="@id/seller_center_createTime"
            android:layout_alignEnd="@id/seller_center_userMessage"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:layout_height="wrap_content" />
    </RelativeLayout>
        <LinearLayout
