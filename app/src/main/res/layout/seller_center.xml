    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="@color/register_text"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
        android:layout_width="0dp"
        android:background="@color/white"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:gravity="center">
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/seller_center_waitingDelivery"
        android:text="6"/>
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="待发货"/>
        </LinearLayout>

        <LinearLayout android:layout_weight="1"
        android:layout_width="0dp"
        android:background="@color/white"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:gravity="center">
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/seller_center_pendingPayment"
        android:text="2"/>
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="待付款"/>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
        android:layout_width="0dp"
        android:background="@color/white"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:gravity="center">
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/seller_center_concernUs"
        android:text="36"/>
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="关注我们"/>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#e7e7e7"/>
    <LinearLayout
        android:background="#e7e7e7"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.example.administrator.xiangou.tool.DrawableTextView
            android:text="店铺管理"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:id="@+id/seller_center_store_management_storemanager"
        app:topDrawable="@mipmap/seller_center_store_management_icon"
        android:layout_height="80dp"
        android:background="@color/white"
        app:topDrawableWidth="30dp"
        app:topDrawableHeight="30dp"
        android:gravity="center" />
        <com.example.administrator.xiangou.tool.DrawableTextView
        android:text="商品管理"
            android:layout_weight="1"
            android:layout_width="0dp"
        app:topDrawable="@mipmap/seller_center_goods_management_icon"
        android:layout_height="80dp"
        android:background="@color/white"
        android:layout_marginStart="1dp"
            android:id="@+id/seller_center_store_management_goodsmanager"
        android:layout_marginEnd="1dp"
        app:topDrawableWidth="30dp"
        app:topDrawableHeight="30dp"
        android:gravity="center"
        />
        <com.example.administrator.xiangou.tool.DrawableTextView
        android:text="订单管理"
            android:layout_width="0dp"
            android:layout_weight="1"
        app:topDrawable="@mipmap/seller_center_order_management_icon"
        android:layout_height="80dp"
            android:id="@+id/seller_center_store_management_ordermanager"
        android:background="@color/white"
        app:topDrawableWidth="30dp"
        app:topDrawableHeight="30dp"
        android:gravity="center"
        />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:background="#e7e7e7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.example.administrator.xiangou.tool.DrawableTextView
        android:layout_width="0dp"
            android:layout_weight="1"
        android:text="数据统计"
            android:id="@+id/seller_center_store_management_totaldata"
        android:layout_marginBottom="1dp"
        app:topDrawable="@mipmap/seller_center_data_management_icon"
        app:topDrawableHeight="30dp"
        android:gravity="center"
        android:layout_marginTop="1dp"
        app:topDrawableWidth="30dp"
        android:layout_height="80dp"
        android:background="@color/white"
        />

        <com.example.administrator.xiangou.tool.DrawableTextView
        android:layout_width="0dp"
            android:layout_weight="1"
        android:text="商品营销"
        app:topDrawable="@mipmap/seller_center_marketing_icon"
        app:topDrawableHeight="30dp"
        android:layout_marginStart="1dp"
            android:id="@+id/seller_center_store_management_goodssales"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
            android:layout_marginTop="1dp"
            android:gravity="center"
        app:topDrawableWidth="30dp"
        android:layout_height="80dp"
        android:background="@color/white"
        />

        <com.example.administrator.xiangou.tool.DrawableTextView
        android:layout_weight="1"
            android:layout_width="0dp"
        android:text="优惠管理"
        app:topDrawable="@mipmap/seller_center_discount_management_icon"
        app:topDrawableHeight="30dp"
        android:gravity="center"
        android:layout_marginTop="1dp"
            android:id="@+id/seller_center_store_management_preferentialmanager"
        android:layout_marginBottom="1dp"
        app:topDrawableWidth="30dp"
        android:layout_height="80dp"
        android:background="@color/white"
        />
    </LinearLayout>
</LinearLayout>