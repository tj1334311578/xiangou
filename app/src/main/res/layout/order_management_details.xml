<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
<!--<include layout="@layout/setting_head"/>-->
    <TextView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="10dp"
    android:paddingTop="5dp"
    android:id="@+id/management_details_status"
    android:paddingBottom="5dp"
    android:textSize="12sp"
    android:textColor="@color/white"
    android:background="@color/textcolor_pink"
    android:text="订单状态：交易成功"/>
    <ListView
        android:layout_width="wrap_content"
        android:layout_below="@id/management_details_status"
        android:paddingStart="15dp"
        android:id="@+id/management_details_list"
        android:layout_height="wrap_content">
    </ListView>
</LinearLayout>